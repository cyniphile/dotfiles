{
    "files.autoSave": "afterDelay",
    "editor.fontSize": 14,
    "editor.wordWrap": "off",
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "vim.leader": ",",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        },
        {
            "before": [
                "<Space>"
            ],
            "after": [
                // "f",
                // "\"",
                "A"
            ]
        },
        {
            "before": [
                "enter"
            ],
            "after": [
                "v",
                "a",
                "f"
            ],
        },
        {
            "before": [
                "<leader>",
                "b",
            ],
            "commands": [
                "editor.debug.action.toggleBreakpoint",
            ],
            "when": [
                "debuggersAvailable && editorTextFocus"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "c"
            ],
            "commands": [
                "workbench.view.scm",
            ],
            "when": [
                "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor && vim.mode != 'Insert'"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "p"
            ],
            "commands": [
                "git.push",
            ],
            "when": [
                "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor && vim.mode != 'Insert'"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "commands": [
                "editor.action.peekDefinition",
            ],
            "when": [
                "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor && vim.mode != 'Insert'"
            ]
        },
        {
            "before": [
                "<leader>",
                "r"
            ],
            "commands": [
                "editor.action.rename",
            ],
            "when": [
                "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor && vim.mode != 'Insert'"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                // "editor.action.openLink"
                "editor.action.revealDefinition"
                // "editor.action.revealDefinitionAside",
            ],
            "when": [
                "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor && vim.mode != 'Insert'"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.files.action.focusFilesExplorer",
            ],
            "when": [
                "editorTextFocus && vim.mode != 'Insert'"
            ],
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<Space>"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "enter"
            ],
            "after": [
                "a",
                "f"
            ],
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        }
    ],
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.formatOnSave": true,
    "workbench.activityBar.visible": true,
    "terminal.external.linuxExec": "",
    "terminal.external.osxExec": "iterm",
    "code-runner.saveFileBeforeRun": true,
    "code-runner.executorMap": {
        "python": "$pythonPath $fullFileName"
    },
    "python.languageServer": "Pylance",
    "workbench.statusBar.visible": true,
    "customizeUI.titleBar": "inline",
    "workbench.useExperimentalGridLayout": true,
    "customizeUI.activityBar": "bottom",
    // hide top right icons
    "customizeUI.stylesheet": {
        ".actions-container[aria-label='Editor actions']": "display: none !important;"
    },
    "search.exclude": {
        "**/*.ipynb_checkpoints": true,
        "**/*.venv": true
    },
    "cSpell.userWords": [
        "alloproteins",
        "amirite",
        "Bijective",
        "bioinformatics",
        "bitshifting",
        "Centricity",
        "dataclass",
        "douchey",
        "elif",
        "engineio",
        "exhaustivity",
        "exomes",
        "FASTA",
        "flamegraph",
        "functools",
        "groupby",
        "hget",
        "hincrby",
        "hmset",
        "hset",
        "iloc",
        "ipykernel",
        "ipython",
        "isin",
        "jupyter",
        "jupyterlab",
        "matplotlib",
        "multithreaded",
        "mypy",
        "numpy",
        "perfplot",
        "plotly",
        "pyclass",
        "pyfunction",
        "pytest",
        "Pythonic",
        "Pythonistas",
        "Schermata",
        "Schiefelbein",
        "strategize",
        "struct",
        "uncentered",
        "usize",
        "Woot",
        "xaxis",
        "yaxis"
    ],
    // "python.linting.flake8Enabled": true,
    // "python.linting.mypyEnabled": true,
    "python.linting.flake8Args": [
        "--ignore=E501"
    ],
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "jupyter.askForKernelRestart": false,
    "jupyter.themeMatplotlibPlots": true,
    "jupyter.addGotoCodeLenses": false,
    "cSpell.enabled": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.xls": "default"
    },
    "remote.extensionKind": {
        "iocave.customize-ui": [
            "ui"
        ],
        "iocave.monkey-patch": [
            "ui"
        ]
    },
    "projectManager.git.baseFolders": [
        "/home/cyniphile/projects/"
    ],
    "workbench.startupEditor": "newUntitledFile",
    "pythonTestExplorer.testFramework": "pytest",
    "python.analysis.typeCheckingMode": "basic",
    "testing.enableCodeLens": false,
    "git.autofetch": true,
    "editor.rulers": [
        79,
        120
    ],
    "window.menuBarVisibility": "toggle",
    "settingsSync.ignoredExtensions": [],
    "python.venvPath": "/home/cyniphile/.cache/pypoetry/virtualenvs/",
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "[python]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "markdown.preview.breaks": true,
    "window.zoomLevel": 1,
    "workbench.enableExperiments": false,
    "emmet.includeLanguages": {
        "rescript": "javascriptreact"
    },
    "[rescript]": {
        "editor.autoClosingBrackets": "always"
    },
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.procMacro.enable": false,
    "rust-analyzer.diagnostics.disabled": [
        "macro-error",
        "unresolved-macro-call"
    ],
    // "mypy.runUsingActiveInterpreter": true,
    "notebook.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "search.useGlobalIgnoreFiles": true,
    "editor.fontFamily": "MesloLGS NF, Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "workbench.editor.untitled.hint": "hidden",
    "terminal.integrated.tabs.enabled": false,
    "terminal.integrated.showExitAlert": false,
    "window.titleBarStyle": "native"
}